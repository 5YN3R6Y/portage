ret=0

[[ -d ${ED} ]] || exit ${ret}

find "${ED}" -type d -name man > "${T}"/prepallman.filelist
while read mandir ; do
	mandir=${mandir#${ED}}
	prepman "${mandir%/man}"
	((ret|=$?))
done < "${T}"/prepallman.filelist

exit ${ret}
