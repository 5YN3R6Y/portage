import sys, os
if len(sys.argv)!=3:
	print "xpak: expecting three arguments."
	sys.exit(1)

try:
	import portage.xpak
except ImportError:
	sys.path.insert(0, "@PORTAGE_BASE@/pym")
	import portage.xpak

portage.xpak.xpak(sys.argv[1],sys.argv[2])
