if [[ $# -ne 2 ]] ; then
	echo "${0}: two arguments needed"
	exit 1
fi

<<<<<<< .working
target="${1}"
linkname="${2}"
[[ ${target:0:1} == "/" ]] && target="${PREFIX}${target}"
exec ln -snf "${target}" "${D}${linkname}"

